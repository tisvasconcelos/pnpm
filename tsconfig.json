{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "jsx": "preserve",
    "allowJs": true,
    "moduleResolution": "node",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "isolatedModules": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
  },
}
